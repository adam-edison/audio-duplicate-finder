{
  "scanPaths": [
    "~",
    "/Volumes"
  ],
  "excludePatterns": [
    "node_modules",
    ".git",
    ".svn",
    ".hg",
    "__pycache__",
    ".venv",
    "venv",
    ".npm",
    ".cargo",
    ".rustup",
    "target",
    ".gradle",
    ".maven",
    "build",
    "dist",
    "Pods",
    ".cocoapods",
    "DerivedData",
    ".idea",
    ".vscode",
    "*.xcodeproj",
    "*.xcworkspace",
    ".docker",
    ".kube",
    ".terraform",
    ".vagrant",
    ".gem",
    ".rbenv",
    ".pyenv",
    ".nvm",
    ".nodenv",
    ".goenv",
    ".sdkman",
    "vendor",
    ".bundle",
    ".composer",
    "bower_components",
    ".pnpm",
    ".yarn",
    "coverage",
    ".nyc_output",
    ".pytest_cache",
    "*.egg-info",
    "*.app",
    "*.framework",
    "*.bundle",
    "*.plugin",
    "*.kext",
    "*.vmwarevm",
    "*.parallels",
    "Virtual Machines",
    "*.photoslibrary",
    "Mail",
    "Library",
    ".Trash",
    ".Trashes",
    ".Spotlight-*",
    ".fseventsd",
    ".DocumentRevisions-V100",
    ".TemporaryItems",
    ".MobileBackups",
    "Backups.backupdb",
    ".apdisk",
    ".localized",
    ".com.apple.*",
    ".cache",
    "tmp",
    "temp",
    "logs",
    ".DS_Store",
    "System",
    "private",
    "usr",
    "bin",
    "sbin",
    "var",
    "etc",
    "opt",
    "cores",
    "dev",
    ".vol",
    ".dropbox",
    ".dropbox.cache",
    "repositories",
    "**/parrot.py*/**"
  ],
  "durationToleranceSeconds": 5,
  "duplicateScoreThreshold": 40,
  "supportedExtensions": [
    "mp3",
    "flac",
    "wav",
    "aac",
    "ogg",
    "m4a",
    "aiff",
    "aif",
    "alac",
    "wma",
    "opus",
    "ape",
    "wv"
  ],
  "duplicateRules": {
    "confidenceThreshold": 90,
    "scoreDifferenceThreshold": 10,
    "weights": {
      "lossless": 50,
      "bitrate": 20,
      "pathPriority": 20,
      "metadataQuality": 10
    },
    "pathPriority": [
      "/Users/aedison/Music",
      "/Users/aedison/Downloads",
      "/Volumes/Blackwing - Macbook Pro Stuff/MainAdamAccount/AlterAccountMirror",
      "/Volumes/Blackwing - Macbook Pro Stuff/MainAdamAccount/Music"
    ]
  }
}